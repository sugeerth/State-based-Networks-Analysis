<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
<link id="favicon" rel="shortcut icon" type="image/png" />
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.6.3/css/font-awesome.min.css">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.12.4/css/bootstrap-select.min.css">
<script src='http://d3js.org/d3.v3.min.js' type="text/javascript"></script>

<style>
    .infoText {
        font-size: 11px;
    }
    body, label, table {
        font-family: "Source Sans Pro", sans-serif;
        font-weight: 200;
    }
    .checkbox label, .radio label {
        font-weight: 200;
    }
    .container {
        margin: 0px;
        padding: 0px;
    }
    ul {
        padding-left: 15px;
    }
    .bulletLessList {
        list-style-type: none;
    }
    .paddedList{
        padding-bottom: 7px;
    }
    table {
        font-size: 12px;
    }
    a:focus, a:hover {
        text-decoration: none;
    }
    a {
        color: #84b4c2;
    }
    .table > tbody > tr > td {
        padding-left: 8px;
        padding-right: 8px;
        padding-top: 3px;
        padding-bottom: 3px;
    }
    .panel-custom {
        border: 1px solid #ec9b88;
        box-shadow: 0 2px 2px rgba(0, 0, 0, 0.3);
    }
    .panel-heading {
        background-color: white;
        text-align: left;
        border-color: transparent;
        margin: 0px
        padding: 6px;
    }
    h5 {
        font-size: 16px;
        margin: 0px;
        color: #337ab7;
        font-weight: 200;
    }
    h4{
        border-bottom: 1px solid #84b4c2;
        color: #5a5a5a;
        font-weight: 200;
    }

    h3{
        border-bottom: 1px solid #ec9b88;
        color: #5a5a5a;
        font-weight: 200;
    }

    .table-striped>tbody>tr:nth-of-type(odd) {
        background-color: #fafcfd;
    }
    .table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
        border-top: 1px solid #e8f1f6;
    }

    html {
        height: 100%;
        box-sizing: border-box;
    }
    body {
        position: relative;
        margin: 0;
        padding-bottom: 3rem;
        min-height: 100%;
    }
    .pageBody {
        margin: 0 auto;
    }
    .footer {
        position: absolute;
        right: 0;
        bottom: 0;
        left: 0;
        padding: 1rem;
        color: #d8d8d8;
        font-size: 10px;
    }


</style>
<script>
    function getUrl() {
        var url = location.origin
        return url
    }
    
    $(document).ready(function(){
        $("#favicon").attr("href", getUrl()+"/images/favicon.png");
        $("#timesumLogo").attr("src", getUrl()+"/images/favicon.png")
    })
    function generateLink(URLPath) {
        return '<a href="'+getUrl()+URLPath+'" target="_blank">'+getUrl()+URLPath+'</a>'
    }
    function displayLink(URLPath, id) {
        $(id).html(generateLink(URLPath))
    }


</script>
